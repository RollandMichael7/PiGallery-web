<%= render partial: "layouts/object_card", 
	locals: { 
		link: "subjects/#{subject.id}",
		image_path: subject.images.sample.thumbnail_path,
		title: subject.name,
		subtitles: [subject.species],
		badges: subject.locations.order(:name),
		max_badges: 5,
		import: subject.imports.order("date(imports.created_at) DESC").first,
	}
%>
